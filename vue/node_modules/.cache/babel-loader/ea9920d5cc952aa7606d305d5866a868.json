{"ast":null,"code":"import axios from \"axios\";\nimport request from \"@/utils/request\";\nexport default {\n  name: 'HomeView',\n  components: {// HelloWorld\n  },\n\n  data() {\n    return {\n      form: {},\n      dialogFormVisible: false,\n      currentPage: 1,\n      total: 10,\n      tableData: []\n    };\n  },\n\n  created() {\n    if (sessionStorage != null) {\n      let str = sessionStorage.getItem(\"item\");\n      this.storages = JSON.parse(str);\n      request.put(\"/api/items\", this.storages).then(res => {\n        console.log(res);\n      });\n    }\n\n    sessionStorage.clear();\n    this.load();\n  },\n\n  methods: {\n    load() {\n      request.get(\"/api/items\").then(res => {\n        console.log(res);\n        this.tableData = res;\n      });\n    },\n\n    addItem() {\n      this.dialogFormVisible = true;\n      this.form = {};\n    },\n\n    handleDelete(row) {\n      console.log(this.form); // debugger\n\n      request.delete(\"/api/items/\" + row.id);\n    },\n\n    handleShip(row) {\n      sessionStorage.setItem(\"item\", JSON.stringify(row));\n      this.$router.push(\"/shipment\");\n    },\n\n    saveItem() {\n      if (this.form.id) {\n        console.log(this.form);\n        request.put(\"/api/items\", this.form).then(res => {\n          console.log(res);\n        });\n      } else {\n        request.post(\"/api/items\", this.form).then(res => {\n          console.log(res);\n        });\n      }\n\n      this.dialogFormVisible = false;\n      this.load();\n    },\n\n    handleEdit(row) {\n      console.log(this.form.id);\n      this.form = JSON.parse(JSON.stringify(row));\n      this.dialogFormVisible = true;\n    },\n\n    handleSizeChange() {},\n\n    handleCurrentChange() {}\n\n  }\n};","map":{"version":3,"mappings":"AAqEA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,OAAP,MAAoB,iBAApB;AAEA,eAAe;EACbC,IAAI,EAAE,UADO;EAEbC,UAAU,EAAE,CACV;EADU,CAFC;;EAKXC,IAAI,GAAE;IACN,OAAM;MACJC,IAAI,EAAC,EADD;MAIJC,iBAAgB,EAAI,KAJhB;MAKJC,WAAU,EAAG,CALT;MAMJC,