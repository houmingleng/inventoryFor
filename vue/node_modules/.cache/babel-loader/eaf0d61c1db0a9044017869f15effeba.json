rence>\n                <el-button type=\"text\">delete</el-button>\n              </template>\n            </el-popconfirm>\n\n          </template>\n        </el-table-column>\n      </el-table>\n      <div style=\"margin: 15px 2px\">\n        <el-pagination\n            v-model:currentPage=\"currentPage\"\n            v-model:page-size=\"pageSize4\"\n            :page-sizes=\"[10, 50, 100, 200]\"\n            :small=\"small\"\n            :disabled=\"disabled\"\n            :background=\"background\"\n            layout=\"total, sizes, prev, pager, next, jumper\"\n            :total=\"total\"\n            @size-change=\"handleSizeChange\"\n            @current-change=\"handleCurrentChange\"\n        />\n\n\n\n\n\n\n  </div>\n<!--    <el-dialog-->\n<!--        v-model=\"dialogVisible\" title=\"Tips\" width=\"30%\">-->\n<!--      <el-form :`model`=\"form\" `label-width`=\"150px\">-->\n<!--        <el-form-item label=\"name\"><el-input v-model=\"form.name\"></el-input></el-form-item>-->\n<!--        <el-form-item label=\"number\"><el-input v-model=\"form.number\"></el-input></el-form-item>-->\n<!--      </el-form>-->\n<!--      <template #footer>-->\n<!--      <span class=\"dialog-footer\">-->\n<!--        <el-button @click=\"dialogVisible = false\">Cancel</el-button>-->\n<!--        <el-button type=\"primary\" @click=\"saveItem\"-->\n<!--        >Confirm</el-button-->\n<!--        >-->\n<!--      </span>-->\n<!--      </template>-->\n<!--    </el-dialog>-->\n    <el-dialog v-model=\"dialogFormVisible\" title=\"Shipping address\">\n      <el-form :model=\"form\">\n        <el-form-item label=\"name\" :label-width=\"formLabelWidth\">\n          <el-input v-model=\"form.name\" autocomplete=\"off\" />\n        </el-form-item>\n        <el-form-item label=\"address\" :label-width=\"formLabelWidth\">\n          <el-input v-model=\"form.address\" autocomplete=\"off\" />\n        </el-form-item>\n        <el-form-item label=\"number\" :label-width=\"formLabelWidth\">\n    